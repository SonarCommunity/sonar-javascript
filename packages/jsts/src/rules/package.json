{
  "type": "module",
  "scripts": {
    "build": "npm ci && npm run compile && (cd lib && npm pack)",
    "compile": "npm run clear && tsc -p tsconfig-plugin.json --noEmit && tsc -p tsconfig-plugin.json --noCheck --module commonjs --moduleResolution node --outDir lib/cjs && tsc -p tsconfig-plugin.json --declaration true --emitDeclarationOnly --outDir lib/types && cp package.package.json lib/package.json",
    "eslint-docs": "npm run compile && eslint-doc-generator && rimraf docs",
    "clear": "rimraf --glob lib/*"
  },
  "dependencies": {
    "@babel/core": "7.25.2",
    "@babel/eslint-parser": "7.25.1",
    "@babel/plugin-proposal-decorators": "7.24.7",
